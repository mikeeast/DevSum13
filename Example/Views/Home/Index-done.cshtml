@model dynamic

@section Scripts {
<script src="/Scripts/Impl/Home/index.js"></script>
<script>
    var vm = new viewModel();
    ko.applyBindings(vm);
</script>
    
}

<div class="row">
    <div class="span6">
        <table class="table table-bordered table-striped">
            <thead>
                <tr>
                    <th>Name</th>
                    <th>Price</th>
                </tr>
            </thead>
            <tbody>
                @foreach (var beer in Model)
                {
                    <tr>
                        <td>@beer.Name</td>
                        <td>@beer.Price</td>
                    </tr>    
                }
            </tbody>
        </table>
    </div>
    <div class="span6">
        <table class="table table-bordered table-striped">
            <thead>
                <tr>
                    <th>Name</th>
                    <th>Price</th>
                </tr>
            </thead>
            <tbody data-bind="foreach: beers">
                <tr>
                    <td data-bind="text: Name"></td>
                    <td data-bind="text: Price"></td>
                </tr>
            </tbody>
        </table>
    </div>
</div>
